<!DOCTYPE html>
<html>

<head>
  <meta charset="utf-8">
  <title>会員一覧</title>
  <link href="{{constant('ECweb\\Bootstrap::ENTRY_URL')}}css/style.css" rel="stylesheet">
</head>

<body>
  <div id = "wrapper">

    <table border="1">
    <tr>
      <th>ID</th>
      <th>お名前</th>
      <th>メール</th>
      <th>性別</th>
      <th>登録日時</th>
    </tr>



  {% for value in dataArr %}
  <tr>
    <td>
      {{value.mem_no}}
    </td>

    <td>
      <span style="font-size:60%;">{{value.family_name_kana}}{{value.first_name_kana}}</span><br>
      {{value.family_name}}{{value.first_name}}
    </td>

    <td>
    {{value.email}}
    </td>

    <td>
    {% if value.sex == '1' %}男性{% elseif value.sex =='2' %}女性
    {% endif %}
    </td>

    <td>
    {{value.regist_date}}
    </td>
  </tr>
  {% endfor %}
</table>


  <div class="cart_in">
  <input type="button" name="back" value="戻る" onclick="history.back(); return false;">
  </div>

</div>
</body>
</html>